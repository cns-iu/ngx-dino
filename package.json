{
    "name": "ngx-dino",
    "version": "0.5.0",
    "description": "Visualization framework and library for Angular 6+",
    "homepage": "https://github.com/cns-iu/ngx-dino",
    "bugs": "https://github.com/cns-iu/ngx-dino/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/cns-iu/ngx-dino.git"
    },
    "license": "MIT",
    "keywords": [
        "angular",
        "visualization",
        "typescript",
        "d3",
        "vega"
    ],
    "author": "Cyberinfrastructure for Network Science Center at Indiana University (http://cns.iu.edu/)",
    "contributors": [
        "Bruce W. Herr II <bherr@indiana.edu> (https://github.com/bherr2)",
        "Daniel Bolin <axbolin@iu.edu> (https://github.com/axdanbol)",
        "Gagandeep Singh <singhgag@indiana.edu> (https://github.com/GagsdName)"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "version-patch": "npm version --no-git-tag-version patch && cd projects/core && for p in core vega-util datatable force-network geomap scatterplot science-map legend; do cd ../$p && npm version --no-git-tag-version patch; done;",
        "build:release": "npm run build:libs-release && npm run build:demo-release",
        "build:lib": "ng build --prod",
        "build:libs-release": "for p in core vega-util datatable force-network geomap scatterplot science-map legend; do npm run build:lib $p; done",
        "build:demo-release": "ng build --prod --aot --base-href /ngx-dino/",
        "publish": "npm run publish:cp-readme && npm run publish:libs",
        "publish:cp-readme": "for p in core vega-util datatable force-network geomap scatterplot science-map legend; do cp projects/$p/LICENSE dist/$p && cp -f projects/$p/README.md dist/$p; done",
        "publish:libs": "cd dist/core && for p in core vega-util datatable force-network geomap scatterplot science-map legend; do cd ../$p && npm publish --access public; done"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.1.0",
        "@angular/cdk": "^6.4.1",
        "@angular/common": "^6.1.0",
        "@angular/compiler": "^6.1.0",
        "@angular/core": "^6.1.0",
        "@angular/forms": "^6.1.0",
        "@angular/http": "^6.1.0",
        "@angular/material": "^6.4.1",
        "@angular/platform-browser": "^6.1.0",
        "@angular/platform-browser-dynamic": "^6.1.0",
        "@angular/router": "^6.1.0",
        "@types/lodash": "^4.14.115",
        "core-js": "^2.5.4",
        "d3-array": "^1.2.1",
        "d3-axis": "^1.0.8",
        "d3-drag": "^1.2.1",
        "d3-force": "^1.1.0",
        "d3-scale": "^2.1.0",
        "d3-selection": "^1.3.0",
        "d3-shape": "^1.2.0",
        "d3-transition": "^1.1.1",
        "d3-zoom": "^1.7.1",
        "immutable": "^3.8.2",
        "lodash": "^4.17.10",
        "material-design-icons": "^3.0.1",
        "rxjs": "^6.2.2",
        "vega": "^4.2.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.7.1",
        "@angular-devkit/build-ng-packagr": "^0.7.1",
        "@angular/cli": "^6.1.1",
        "@angular/compiler-cli": "^6.1.0",
        "@angular/language-service": "^6.1.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^10.5.4",
        "codelyzer": "~4.4.2",
        "jasmine-core": "~3.1.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^2.0.5",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.0",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^1.2.0",
        "ng-packagr": "^4.0.1",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": "0.32.0",
        "tslib": "^1.7.1",
        "tslint": "~5.11.0",
        "typescript": "~2.9.2"
    }
}
